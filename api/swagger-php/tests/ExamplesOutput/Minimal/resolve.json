{
    "basePath": "http://example.com/api",
    "swaggerVersion": "1.2",
    "apiVersion": "0.2",
    "resourcePath": "/resolve",
    "apis": [
        {
            "path": "/resolve",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Retrieve all dogs",
                    "nickname": "get_dogs",
                    "type": "List[Dog]"
                }
            ]
        }
    ],
    "models": {
        "Dog": {
            "id": "Dog",
            "description": "A dog. a.k.a. Canis lupus familiaris",
            "required": ["breed"],
            "properties": {
                "breed": {
                    "type": "string"
                },
                "parent": {
                    "$ref": "Dog"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "$ref": "Tag"
                    }
                },
                "id": {
                    "type": "integer"
                },
                "category": {
                    "$ref": "Category"
                },
                "status": {
                    "type": "string",
                    "description": "Pet status in the store",
                    "enum":[
                        "available",
                        "pending",
                        "sold"
                    ]
                },
                "name": {
                    "type": "string"
                },
                "photoUrls": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}