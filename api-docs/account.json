{
    "basePath": "http://api.castcloud.org/api",
    "swaggerVersion": "1.2",
    "apiVersion": "1.0.0",
    "resourcePath": "/account",
    "apis": [
        {
            "path": "/account/login",
            "description": "User login. Returns tolken required for accessing the rest of the api.",
            "operations": [
                {
                    "method": "POST",
                    "nickname": "Login",
                    "parameters": [
                        {
                            "paramType": "body",
                            "name": "username",
                            "type": "string",
                            "required": true,
                            "description": "ID of pet that needs to be fetched"
                        }
                    ]
                }
            ]
        }
    ],
    "produces": [
        "application/json",
        "application/xml"
    ]
}