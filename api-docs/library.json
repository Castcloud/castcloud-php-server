            "properties": {
                "name": {
                    "description": "The casts individual ID",
                    "type": "integer",
                    "format": "int64"
                },
                "casts": {
                    "description": "The casts url",
                    "type": "string"
                },
                "description": {
                    "description": "The casts url",
                    "type": "string"
                }
            },
            "required": [
                "feed",
                "id",
                "tags",
                "url"
            ]
        }
