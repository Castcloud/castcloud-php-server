    "models": {
        "newepisodesresult": {
            "id": "newepisodesresult",
            "properties": {
                "timestamp": {
                    "description": "Timestamp for the request",
                    "type": "integer",
                    "format": "int64"
                },
                "episodes": {
                    "description": "Array of all the episodes",
                    "type": "array",
                    "items": {
                        "$ref": "episode"
                    }
                }
            },
            "required": [
                "episodes",
                "timestamp"
            ]
        },
        "episode": {
            "id": "episode",
            "properties": {
                "id": {
                    "description": "Unique identifier for the episode",
                    "type": "integer",
                    "format": "int64"
                },
                "castid": {
                    "description": "Unique identifier for the cast related to the episode",
                    "type": "integer",
                    "format": "int64"
                },
                "lastevent": {
                    "description": "The episodes last event",
                    "$ref": "event"
                },
                "feed": {
                    "description": "All data available in the episodes item",
                    "type": "array"
                }
            },
            "required": [
                "castid",
                "id"
            ]
        },
        "event": {
            "id": "event",
            "properties": {
                "type": {
                    "description": "The event type",
                    "type": "integer",
                    "format": "int64"
                },
                "episodeid": {
                    "description": "The id of the episode it relates to",
                    "type": "integer",
                    "format": "int64"
                },
                "positionts": {
                    "description": "The position where the event occured",
                    "type": "integer",
                    "format": "int64"
                },
                "clientts": {
                    "description": "The clients unix timestamp of when the event occured",
                    "type": "integer",
                    "format": "int64"
                },
                "concurrentorder": {
                    "description": "If multiple events occure on the same time, This wil decribe the order of the events.",
                    "type": "integer",
                    "format": "int64"
                },
                "clientname": {
                    "description": "Name of the client that sent the event",
                    "type": "string"
                },
                "clientdescription": {
                    "description": "Description of the client that sent the event",
                    "type": "string"
                }
            },
            "required": [
                "clientts",
                "episodeid",
                "positionts",
                "type"
            ]
        },
        "cast": {
            "id": "cast",
            "properties": {
                "id": {
                    "description": "The casts individual ID",
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "description": "The casts display name",
                    "type": "string"
                },
                "url": {
                    "description": "The casts url",
                    "type": "string"
                },
                "feed": {
                    "description": "All feed related data from the feed",
                    "type": "array"
                }
            },
            "required": [
                "feed",
                "id",
                "name",
                "url"
            ]
        },
        "opml": {
            "id": "opml",
            "properties": {
                "title": {
                    "description": "The casts individual ID",
                    "type": "string"
                },
                "dateCreated": {
                    "description": "The casts url",
                    "type": "string"
                },
                "tags": {
                    "description": "All feed related data from the feed",
                    "type": "array",
                    "items": {
                        "$ref": "opml_tag"
                    }
                },
                "ownerEmail": {
                    "description": "All feed related data from the feed",
                    "type": "string"
                },
                "untagged": {
                    "description": "The casts url",
                    "type": "array",
                    "items": {
                        "$ref": "opml_cast"
                    }
                }
            },
            "required": [
                "feed",
                "id",
                "tags",
                "url"
            ]
