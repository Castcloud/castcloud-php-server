                    "nickname": "Get users subcriptions",
                    "type": "Herp",
                    "parameters": [
                        {
                            "paramType": "header",
                            "name": "Authorization",
                            "type": "string",
                            "required": true,
                            "description": "clients login token"
                        }
                    ]
                },
